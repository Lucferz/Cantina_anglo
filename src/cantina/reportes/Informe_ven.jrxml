	<queryString>
		<![CDATA[SELECT
     ventas.`idventa` AS ID,
     ventas.`id_caja` AS CAJA,
     ventas.`fecha` AS FECHA,
     ventas.`total` AS TOTAL,
     usuarios.`nombre` AS USUARIO
FROM
     `ventas` ventas, `usuarios` usuarios
WHERE
	ventas.`estado` = 1 AND
	ventas.`fk_usuario` = usuarios.`idusuario` AND
	ventas.`fecha` BETWEEN TIMESTAMP($P{fecha_ini}) AND TIMESTAMP($P{fecha_fin})
ORDER BY ventas.`idventa` asc]]>
	</queryString>
